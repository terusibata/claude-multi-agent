{
  "id": null,
  "uid": "workspace-containers",
  "title": "Workspace Containers",
  "version": 1,
  "schemaVersion": 39,
  "timezone": "browser",
  "editable": true,
  "refresh": "15s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "type": "row",
      "title": "Container Lifecycle",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Active Containers",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 15 },
              { "color": "red", "value": 25 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "workspace_active_containers",
          "legendFormat": "active",
          "refId": "A"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Startup Time P95",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "s",
          "min": 0,
          "max": 20
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(workspace_container_startup_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests/sec by Status",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate(workspace_requests_total[5m])",
          "legendFormat": "{{ status }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "WarmPool",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "gauge",
      "title": "Pool Size",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 2 },
              { "color": "green", "value": 5 }
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 20
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "workspace_warm_pool_size",
          "legendFormat": "pool size",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Exhaustion Rate",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(workspace_warm_pool_exhausted_total[5m])",
          "legendFormat": "exhaustion rate",
          "refId": "A"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Acquire Time P95",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          },
          "unit": "s",
          "min": 0,
          "max": 10
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(workspace_warm_pool_acquire_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "Proxy",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Request Duration P95",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 15 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(workspace_proxy_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Blocked Requests Rate",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 15 },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(workspace_proxy_blocked_requests_total[5m])",
          "legendFormat": "blocked/sec",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Throughput",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 15 },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(workspace_proxy_bytes_transferred_total[5m])",
          "legendFormat": "{{ direction }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "Security & GC",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 21 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "GC Cycles",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 22 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "lineWidth": 1
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "increase(workspace_gc_cycles_total[5m])",
          "legendFormat": "{{ result }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Seccomp Violations",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 22 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(workspace_seccomp_violations_total[5m])",
          "legendFormat": "violations/sec",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Crash Rate",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 22 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.05 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(workspace_container_crashes_total[5m]) / rate(workspace_requests_total[5m])",
          "legendFormat": "crash rate",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] }
}
